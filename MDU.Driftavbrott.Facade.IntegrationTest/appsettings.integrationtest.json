{
    "Logging": {
        "LogLevel": {
            "Default": "Debug"
        },
        "NLog": {
            "autoReload": true,
            "IncludeScopes": false,
            "LogLevel": {
                "Default": "Debug",
                "System": "Information",
                "Microsoft": "Information"
            },
            "Targets": {
                "File": {
                    "Type": "File",
                    "FileName": "MDU.Driftavbrott.Facade.IntegrationTest.log",
                    "Layout": "${longdate} ${level:uppercase=true} [${trace}] ${logger}: ${message}"
                },
                "Coloredconsole": {
                    "Type": "ColoredConsole"
                }
            },
            "Rules": [
                {
                    "logger": "*",
                    "minLevel": "Debug",
                    "writeTo": "file, coloredconsole"
                }
            ]
        }
    },
    "DriftavbrottFacadeSettings": {
        "Url": "http://integration-linux-dev.ita.mdh.se:3301/mdh-driftavbrott/v1",
        "Kanaler": [
            "alltid"
        ],
        "System": "MDU.Driftavbrott.Facade.IntegrationTest",
        "MonitorIntervalInSeconds": 3
    }
}